<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>Edit User</title>

  <% include ../partials/header %>
  <% include ../partials/admin-sidebar %>


  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        Dashboard
        <small>Welcome!</small>
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Level</a></li>
        <li class="active">Here</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content container-fluid">

      <!--------------------------
        | Your Page Content Here |
        -------------------------->

          <div class="box box-primary">
            <div class="box-header with-border">
              <h3 class="box-title">Update Manufacturer</h3>
            </div>
            <!-- /.box-header -->
            <!-- form start -->
            <form method="post">
              <div class="box-body">
                <div class="form-group">
                    <label>Manufacturer ID</label>
                    <input type="text" class="form-control" id="hotel-title" placeholder="Name" name="man_id" value="<%=result.man_id%>" readonly>
                  </div>
                  <div class="form-group">
                      <label>Manufacturer Name</label>
                      <input type="text" class="form-control" id="hotel-title" placeholder="Name" name="man_name" value="<%=result.man_name%>">
                    </div>
                    <div class="form-group">
                        <label>Manufacturer Address</label>
                        <input type="textarea" class="form-control" id="Username" placeholder="username" name="man_address" value="<%=result.man_address%>" >
                    </div>
                    <div class="form-group">
                      <label>Manufacturer License</label>
                      <input type="text" class="form-control" id="contact" placeholder="contact" name="man_license" value="<%=result.man_license%>" readonly>
                  </div>
                <div class="error">
                <% if(message != null){ %>
                <p><span id="error"><%= message %></span></p>
                <% } %>
                </div>	

                <button type="submit" class="btn btn-primary">UPDATE</button>
              </div>
            </form>
          </div>

    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->