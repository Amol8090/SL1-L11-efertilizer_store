        -------------------------->

          <div class="box box-primary">
            <div class="box-header with-border">
              <h3 class="box-title">Add New</h3>
            </div>
            <!-- /.box-header -->
            <!-- form start -->
            <form method="post">
              <div class="box-body">

                <div class="form-group">
                  <label>Name</label>
                  <input type="text" class="form-control" id="hotel-title" placeholder="Name" name="name" value="<%=result.user_name%>">
                </div>
                <div class="form-group">
                    <label>Username</label>
                    <input type="text" class="form-control" id="Username" placeholder="username" name="username" value="<%=result.user_username%>" readonly>
                </div>
                <div class="form-group">
                  <label>Contact</label>
                  <input type="text" class="form-control" id="contact" placeholder="contact" name="contact" value="<%=result.user_contact%>">
              </div>
                <div class="form-group">
                    <label>Email</label>
                    <input type="text" class="form-control" id="email" placeholder="email" name="email" value="<%=result.user_email%>">
                </div>
                <div class="form-group">
                  <label>Password</label>
                  <input type="text" class="form-control" id="Password" placeholder="Password" name="password" value="<%=result.user_password%>">
              </div>

                <button type="submit" class="btn btn-primary">UPDATE</button>
              </div>
            </form>
          </div>

    </section>
