      <% var count = 0; %>
      <% var numRows = Math.ceil(orderList.length/4); %>
      <!-- <% console.log("===>", numRows); %> -->
      <% for(var i=0; i < numRows; i++){ %>
        <div class="row">
          <% for(var j=0; j < 4; j++){ %>
            <% if(count < orderList.length){ %>
              <div class="col-4">
                <div style="border: green solid 2px;">
                  <% if(orderList[count].availability) {%>
                    <a href="/userDashboard/product/<%= orderList[count].fert_id %>" ><img src="<%= orderList[count].image %>" alt=""></a>
                  <% }else{ %>
                    <img src="<%= orderList[count].image %>" alt="">
                  <% } %>
                <div style="text-align: center; width: 100%; ">
                  <h1><%= orderList[count].fert_name %></h1>
                  <p><strong>Quantity Ordered : <%= orderList[count].quantity %></strong></p>
                  <p><strong>Total Cost : Rs. <%= orderList[count].total_cost %></strong></p>
                  <p><strong>Date : <%= String(orderList[count].orderDate).split(" ")[1] + " " + String(orderList[count].orderDate).split(" ")[2] + " " + String(orderList[count].orderDate).split(" ")[3] %></strong></p>
                  <% if(orderList[count].availability == 0) { %>
                    <p><strong>Status : <span style="color: red;" >Out of Stock</span></strong></p>
                  <% } else{ %>
                    <p><strong>Status : <span style="color: green;" >Available</span></strong></p>
                  <% } %>

                  <!-- <p><strong>Date : <%= orderList[count].orderDate %></strong></p> -->
                </div>
                </div>
              </div>
              <% count++; %>
            <% } %>
          <% } %>
        </div>
      <% } %>


  
